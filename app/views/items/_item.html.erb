<div class="media">
  <div class="media-body">
    <h4 class="media-heading">
        <%=item.name %>
    </h4>
    <small>
      submitted <%= time_ago_in_words(item.created_at) %> ago. <br>
    </small>
    <%= link_to "Item Complete", [current_user, item], method: :delete, class: 'btn btn-danger',
    data: { confirm: 'Are you sure you want to delete this item?' } %>
  </div>
</div>
